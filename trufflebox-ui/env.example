# Horizon Backend Service URLs
# Base URL for Horizon API service (development/staging)
REACT_APP_HORIZON_BASE_URL=http://localhost:8082

# Production Horizon API service URL (used for promotion workflows)
REACT_APP_HORIZON_PROD_BASE_URL=http://localhost:8085

# Skye Service URL
REACT_APP_SKYE_BASE_URL=http://localhost:8083

# Model Inference Service URL
REACT_APP_MODEL_INFERENCE_BASE_URL=http://localhost:8084

# Application Environment
# Options: production, staging, development
REACT_APP_ENVIRONMENT=production

# Service-wise Feature Flags
# Enable/disable specific services (true/false or 1/0)
# Online Feature Store - Default: enabled
REACT_APP_ONLINE_FEATURE_STORE_ENABLED=true

# InferFlow (Model Proxy) - Default: disabled
REACT_APP_INFERFLOW_ENABLED=true

# Numerix - Default: enabled
REACT_APP_NUMERIX_ENABLED=true

# Predator - Default: disabled
REACT_APP_PREDATOR_ENABLED=true

# Embedding Platform - Default: disabled
REACT_APP_EMBEDDING_PLATFORM_ENABLED=false

# Public Base URL
# Base path for React Router (used when app is deployed under a subpath)
PUBLIC_USER_BASE_URL=/
